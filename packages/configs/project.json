{
  "name": "configs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/configs/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "nx-release-publish": {
      "executor": "@nx/js:release-publish",
      "options": {
        "buildTarget": "configs:build",
        "outputPath": "dist/configs",
        "registry": "https://npm.pkg.github.com",
        "tag": "next"
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/configs",
        "main": "packages/configs/src/index.ts",
        "tsConfig": "packages/configs/tsconfig.lib.json",
        "assets": ["packages/configs/*.md"]
      }
    }
  }
}
